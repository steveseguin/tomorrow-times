	function fn_nextAwesomeitem(sliderNo, ) {

		var viewCnt = mob_viewCnt;

		if (!maxWidth.matches) { // not mobile
			viewCnt = page_viewCnt;

		}
		var data = {
			awesomeslide_no: sliderNo,
			awesome_offset: viewCnt
		};
		$.get("/index.php?op=ajax", data, function(resp) {
			if (resp) {
				var ind_1 = resp.indexOf("<deal_total_start>");
				var ind_2 = resp.indexOf("<deal_total_end>");
				//alert("1="+ind_1+"; ind2="+ind_2);
				if (ind_1 >= 0 && ind_2 > ind_1 + 18) { //has deal info. not black Friday
					var total_str = resp.substring(ind_1 + 18, ind_2);
					var deal_total = parseInt(total_str);
					//alert("total="+total_str);
					var row_counter_pow1 = 0;
					if (maxWidth.matches) {
						row_counter_pow1 = Math.floor(deal_total / viewCnt);
						//mob_row_counter_pow1 = Math.floor(totalPowercnt/ 2);

					} else {

						row_counter_pow1 = Math.floor(deal_total / viewCnt);
						if (row_counter_pow1 >= 5) {
							row_counter_pow1 = 5;
						}
					}
					var row_counter_awe1 = row_counter_pow1;
					//alert("709="+row_counter_awe1);
					/*
										var row_counter_awe1 = Math.floor(deal_total / 10);
								  	  	if(row_counter_awe1 >=5){
									  		row_counter_awe1 = 5;
									  	}
										var deal_page_str = '';
					*/
					for (l = 1; l <= row_counter_awe1; l++) {
						if (l == 1) {
							deal_page_str = '<button class="button-awe button_awesome_' + l + ' is-selected-awe" onclick="fn_nextAwesomeitem(' + l + ')">' + l + '</button>';
						} else {
							deal_page_str = deal_page_str + '<button class="button-awe button_awesome_' + l + '" onclick="fn_nextAwesomeitem(' + l + ');">' + l + '</button>';
						}

					}
					$("#awesomesavingSlider").html(deal_page_str);
					$(".awesome_total").html(total_str);
					var rest = resp.substring(ind_2 + 16);
					resp = rest;
					//alert("607="+resp);
				}


				$('#awesomesavingDiv').empty();
				resp = '<div class="d-flex flex-wrap w-100">' + resp + '</div>';

				if (sliderNo == 1) {
					$('#button_previous_awesome').attr("disabled", true);
				} else {
					$('#button_previous_awesome').attr("disabled", false);
				}
				if (sliderNo >= row_counter_awe1) {
					$('#button_next_awesome').attr("disabled", true);
				} else {
					$('#button_next_awesome').attr("disabled", false);
				}
				$('#button_previous_awesome').attr('onClick', 'fn_nextAwesomeitem(' + (sliderNo - 1) + ');');
				$('#button_next_awesome').attr('onClick', 'fn_nextAwesomeitem(' + (sliderNo + 1) + ');');
				//$('#awesomesavingDiv').append(resp);
				$('#awesomesavingDiv').html(resp);
				$('#awesomesavingSlider').find('.button-awe').removeClass("is-selected-awe");
				$('#awesomesavingSlider').find('.button_awesome_' + sliderNo).addClass("is-selected-awe");
			}
		});
	}
